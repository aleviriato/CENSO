create table TB_AGENCIA
(
  num_agencia     NUMBER(19) not null,
  cod_banco       NUMBER(10),
  cod_ins         NUMBER(10),
  dat_ing         TIMESTAMP(6),
  dat_ult_atu     TIMESTAMP(6),
  des_agencia     VARCHAR2(255 CHAR),
  nom_pro_ult_atu VARCHAR2(255 CHAR),
  nom_usu_ult_atu VARCHAR2(255 CHAR),
  num_dv_agencia  VARCHAR2(255 CHAR)
)
;
alter table TB_AGENCIA
  add primary key (NUM_AGENCIA);

