create table TB_AGENDAMENTO_PENDENTES
(
  id               NUMBER(19) not null,
  dat_agendamento  TIMESTAMP(6),
  hora_inicio      VARCHAR2(5),
  hora_final       VARCHAR2(5),
  pessoa_fisica_id NUMBER(19),
  dat_criacao      TIMESTAMP(6),
  dat_ult_atu      TIMESTAMP(6),
  nom_usu_criacao  VARCHAR2(255),
  nom_usu_ult_atu  VARCHAR2(255)
)
;
alter table TB_AGENDAMENTO_PENDENTES
  add constraint TB_AGENDAMENTO_PENDENTES_PK primary key (ID);

