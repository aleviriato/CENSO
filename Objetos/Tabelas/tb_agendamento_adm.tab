  flg_aniver_mar NUMBER(1) default 1,
  flg_aniver_abr NUMBER(1) default 1,
  flg_aniver_mai NUMBER(1) default 1,
  flg_aniver_jun NUMBER(1) default 1,
  flg_aniver_jul NUMBER(1) default 1,
  flg_aniver_ago NUMBER(1) default 1,
  flg_aniver_set NUMBER(1) default 1,
  flg_aniver_out NUMBER(1) default 1,
  flg_aniver_nov NUMBER(1) default 1,
  flg_aniver_dez NUMBER(1) default 1,
  local_id       NUMBER(19)
