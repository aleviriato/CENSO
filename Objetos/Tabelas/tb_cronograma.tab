create table TB_CRONOGRAMA
(
  id                    NUMBER(11) not null,
  mes_aniversario       VARCHAR2(2) not null,
  data_inicio           DATE not null,
  data_fim              DATE not null,
  data_ini_acesso_agend DATE,
  data_fim_acesso_agend DATE,
  dat_criacao           TIMESTAMP(6),
  dat_ult_atu           TIMESTAMP(6),
  nom_usu_criacao       VARCHAR2(255),
  nom_usu_ult_atu       VARCHAR2(255)
)
;
alter table TB_CRONOGRAMA
  add constraint TB_CRONOGRAMA_PK primary key (ID);

